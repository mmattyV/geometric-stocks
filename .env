# Enable M2 chip optimization for PyTorch
export PYTORCH_ENABLE_MPS_FALLBACK=1

# Set default random seed for consistency
export PYTHONHASHSEED=42

# Environment variables for PyTorch Geometric
export TORCH_GEOMETRIC_HANDLE_DATA_LEAFS=1

# Add alias for M2-optimized Python (this won't be set by source)
# Use this separately: alias python="arch -arm64 python3"
